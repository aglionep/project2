<%= form_for [@review.field, @review] do |f| %>

	<div class="field">
		<%= f.label :comments %> <br>
		<%= f.text_area :comments, size: "40x6" %>
	</div>

	<div class="rating">
		<%= f.label :rating %> <br>
		<%= f.text_field :rating %>
	</div>
	
	<div class="actions">
			<%= f.submit %>
	</div>
<% end %>
