<h2>Add a new Field </h2>

<%= form_for @field do |f| %>
	<div>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>

	<div>
		<%= f.label :team %>
		<%= f.text_field :team %>
	</div>

	<div>
		<%= f.label :league %>
		<%= f.text_field :league %>
	</div>

	<div>
		<%= f.label :address %>
		<%= f.text_field :address %>
	</div>

	<%= f.submit "Add a new Field!" %>

<% end %>

<%= link_to "Back to list", fields_path %>